    "mass": { "variable": "pair_mass", "pretty": "m(#mu#mu) (GeV)" },
    "mcMass": { "variable": "pair_mass", "pretty": "m(#mu#mu) (GeV)" }
  },
  "fitVariable": "mass",
  "fitVariableGen": "mcMass",
